{
  "expo": {
    "name": "AI Doctor App",
    "slug": "ai-doctor-app",
    "scheme": "aidoctor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#E8EFF7"
    },
    "ios": { "supportsTablet": true },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#E8EFF7"
      }
    },
    "web": { "bundler": "metro", "output": "static" },
    "plugins": ["expo-router"]
  }
}
